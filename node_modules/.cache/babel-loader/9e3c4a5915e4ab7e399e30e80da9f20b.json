     type: String,\n      require: true,\n    },\n    icon: {\n      type: String,\n      require: true,\n    },\n    url: {\n      require: true,\n    },\n    hasGroup: {\n      type: Boolean,\n      require: true,\n      default: false,\n    },\n    collapsed: {\n      type: Boolean,\n      require: true,\n      default: true,\n    },\n    id: {\n      type: Number,\n      require: false,\n    },\n  },\n  computed: {\n    isCurrent() {\n      return (\n        this.$route.params.id == this.url ||\n        this.$route.params.bookId == this.url\n      );\n    },\n    isCurrentCategory() {\n      // active router case '評価が高いランキング'\n      if (\n        this.name == \"評価が高いランキング\" &&\n        this.url == `${window.companyPath}bookshelf/list-book` &&\n        this.$route.params.id == undefined\n      )\n        return true;\n      // active router case 'カテゴリー'\n      if (this.name == \"カテゴリー\" && this.$route.params.id != undefined)\n        return true;\n      return false;\n    },\n    isActive() {\n      return window.location.href == this.url;\n    },\n  },\n  methods: {\n    redirect(url, isOpenNewTab) {\n      if (this.id == null) {\n        if (isOpenNewTab == true) {\n          window.open(url, \"_blank\");\n        } else if (this.hasGroup || !this.checkUrlItem(url)) {\n          if (this.checkUrlStatic(url)) {\n            let routeData = this.$router.resolve({\n              path: `/main-bookshelf/statistical-page`,\n            });\n            window.open(routeData.href, \"_blank\");\n          } else window.location = url;\n        }\n      } else {\n        this.$emit(\"onRedirect\");\n      }\n    },\n    showSubMenu() {\n      this.$emit(\"handleClickShowSubMenu\");\n    },\n    checkUrlItem(url) {\n      return url.includes(\"undefined\");\n    },\n    checkUrlStatic(url) {\n      return url.includes(\"statistical-page\");\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n@import \"@/share/assets/scss/variables.scss\";\n\n.menu-item {\n  display: flex;\n  align-items: center;\n  padding: 12px 0;\n  height: 48px;\n  cursor: pointer;\n  width: 100%;\n  @include mediaMaxWidth($mobile) {\n    height: unset;\n  }\n  .menu-item__link {\n    display: flex;\n    align-items: center;\n    width: 100%;\n  }\n  .menu-item__toggle {\n    display: flex;\n    align-items: center;\n    transform: rotate(-90deg);\n    @media (max-width: 415px) {\n      position: absolute;\n      right: 0;\n    }\n    .icon {\n      display: flex;\n      align-items: center;\n      background-size: 24px;\n      height: 24px;\n      width: 24px;\n    }\n  }\n  .menu-item__toggle.collapsed {\n    transform: rotate(0deg);\n  }\n  .current-menu-item {\n    font-weight: bold;\n    font-size: 16px;\n    color: #007bc3;\n  }\n  &.has-group {\n    .menu-item__link {\n      width: calc(100% - 24px);\n    }\n    .menu-item__toggle {\n      display: flex;\n      align-items: center;\n    }\n  }\n  .icon,\n  .icon i {\n    background-size: 24px 24px;\n    width: 24px;\n    height: 24px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n  }\n  .name {\n    font-size: 16px;\n    display: inline-block;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n    @include mediaMaxWidth($mobile) {\n      overflow: hidden;\n      text-overflow: ellipsis;\n      white-space: nowrap;\n    }\n  }\n  &.active {\n    color: #007bc3;\n    background-color: #f4f7fb;\n  }\n  &:hover {\n    color: #007bc3;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}