>\n    <div\n      v-for=\"(menu, menuIndex) in menus\"\n      :key=\"menuIndex\"\n      :class=\"classData(menu)\"\n    >\n      <div class=\"sidebar-title\" v-if=\"menu.title\">\n        <div class=\"icon\" v-if=\"menu.icon\">\n          <i :class=\"menu.icon ? menu.icon : ''\"></i>\n        </div>\n        <div class=\"name\" :title=\"menu.title\">\n          <span>{{ menu.title }}</span>\n        </div>\n      </div>\n      <div\n        v-for=\"(submenu, subMenuIndex) in menu.items\"\n        :key=\"subMenuIndex\"\n        style=\"width: 100%\"\n      >\n        <PageNavigationItem\n          :name=\"submenu.name\"\n          :url=\"submenu.url\"\n          :icon=\"submenu.iconName\"\n          :hasGroup=\"submenu.items && submenu.items.length > 0\"\n          :collapsed=\"submenu.collapse\"\n          @handleClickShowSubMenu=\"\n            showSubMenu(menuIndex, subMenuIndex, -1, submenu.collapse)\n          \"\n          :id=\"null\"\n        />\n        <AnimationFadeUp>\n          <div\n            v-if=\"!!submenu.items\"\n            style=\"padding-left: 12px\"\n            v-show=\"submenu.collapse\"\n          >\n            <div\n              v-for=\"(submenu1, subMenuIndex1) in submenu.items\"\n              :key=\"subMenuIndex1\"\n            >\n              <PageNavigationItem\n                :name=\"submenu1.name\"\n                :url=\"submenu1.id\"\n                :icon=\"submenu1.iconName\"\n                :hasGroup=\"submenu1.items && submenu1.items.length > 0\"\n                :collapsed=\"submenu1.collapse\"\n                :id=\"submenu1.id\"\n                @onRedirect=\"changeCategoryPage(submenu1.id)\"\n              />\n            </div>\n          </div>\n        </AnimationFadeUp>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport PageNavigationItem from \"./PageNavigationItem.vue\";\nimport AnimationFadeUp from \"./AnimationFadeUp\";\nexport default {\n  name: \"SideBarMenu\",\n  data() {\n    return {\n      theme: \"white-theme\",\n      freeLocationMenu: [],\n      menus: [\n        {\n          header: false,\n          title: \"Menu\",\n        },\n        {\n          href: \"/\",\n          title: \"Dashboard\",\n          icon: \"fa fa-user\",\n        },\n        {\n          href: \"/button\",\n          title: \"Button\",\n          icon: \"fa fa-chart-area\",\n        },\n      ],\n    };\n  },\n  components: { PageNavigationItem, AnimationFadeUp },\n  methods: {\n    async getListSidebar() {\n      // let url = `${window.companyPath}`;\n      let menuSidebar = {\n        meta: {\n          taskCount: 2,\n        },\n        data: {\n          title: \"みんなの本棚\",\n          categories: [\n            {\n              name: \"評価が高いランキング\",\n              url: `${window.companyPath}bookshelf/list-book`,\n            },\n            {\n              name: \"カテゴリー\",\n              menus: [],\n              url: \"\",\n            },\n            // {\n            //   name: '新入社員にお勧めの書籍',\n            // },\n            // {\n            //   name: '管理職にお勧めの書籍',\n            // },\n            {\n              name: \"サマリー\",\n              url: `${window.companyPath}bookshelf/main-bookshelf/statistical-page`,\n            },\n          ],\n        },\n      };\n      // get sub-category\n      // const response = await api('getListSidebarApi')\n      // this.$store.commit('categories/SET_LIST_CATEGORIES', response.data?.data)\n      // if (response.data.data) {\n      //   menuSidebar.data.categories[1].menus = response.data.data\n      //   menuSidebar.data.categories[1].url = `${window.companyPath}bookshelf/list-book/${response.data.data[0]?.id}`\n      // }\n      this.titleSidebar = menuSidebar.data.title;\n      this.freeLocationMenu = menuSidebar.data.categories.map((item) => {\n        return {\n          name: \"\",\n          menuLocation: [item],\n        };\n      });\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}