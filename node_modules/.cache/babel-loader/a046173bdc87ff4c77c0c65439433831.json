stSidebar() {\n      let menuSidebar = {\n        meta: {\n          taskCount: 2\n        },\n        data: {\n          categories: [{\n            name: \"Menu\",\n            menus: [{\n              url: ``,\n              name: \"Dashboard\",\n              iconName: \"fa fa-user\"\n            }, {\n              url: `/button`,\n              name: \"Button\",\n              iconName: \"fa fa-chart-area\"\n            }],\n            url: \"\"\n          }]\n        }\n      };\n      console.log(this.$route); // get sub-category\n\n      this.titleSidebar = menuSidebar.data.title;\n      this.freeLocationMenu = menuSidebar.data.categories.map(item => {\n        return {\n          name: \"\",\n          menuLocation: [item]\n        };\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqEA,OAAOA,kBAAP,MAA+B,0BAA/B;AACA,OAAOC,eAAP,MAA4B,mBAA5B;AACA,eAAe;EACbC,IAAI,EAAE,aADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,gBAAgB,EAAE,EADb;MAELC,KAAK,EAAE;IAFF,CAAP;EAID,CAPY;;EAQbC,UAAU,EAAE;IAAEN,kBAAF;IAAsBC;EAAtB,CARC;;EASbM,OAAO,GAAG;IACR,KAAKC,cAAL;IACA,KAAKC,UAAL;EACD,CAZY;;EAabC,OAAO,EAAE;IACPC,SAAS,CAACC,GAAD,EAAM;MACb,IAAIA,GAAG,CAACC,KAAJ,IAAa,uBAAb,IAAwCD,GAAG,CAACC,KAAJ,IAAa,WAAzD,EACE,OAAO,mBAAP;IACH,CAJM;;IAKPC,kBAAkB,CAACC,GAAD,EAAM;MACtBC,OAAO,CAACC,GAAR;MACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAAEC,IAAI,EAAEL;MAAR,CAAlB;IACD,CARM;;IASPM,WAAW,CAACC,SAAD,EAAYC,YAAZ,EAA0BC,aAA1B,EAAyCC,MAAzC,EAAiD;MAC1D,IAAID,aAAY,IAAK,CAAC,CAAtB,EAAyB;QACvB,KAAKnB,KAAL,CAAWiB,SAAX,EAAsBI,KAAtB,CAA4BH,YAA5B,EAA0CI,QAA1C,GAAqD,CAACF,MAAtD;MACF,CAFA,MAEO;QACL,KAAKpB,KAAL,CAAWiB,SAAX,EAAsBI,KAAtB,CAA4BH,YAA5B,EAA0CG,KAA1C,CACEF,aADF,EAEEG,QAFF,GAEa,CAACF,MAFd;MAGF;IACD,CAjBM;;IAkBPhB,UAAU,GAAG;MACX,IAAImB,SAAQ,GAAI,EAAhB;;MACA,IAAI,CAAC,KAAKxB,gBAAV,EAA4B;QAC1B;MACF;;MACA,KAAKA,gBAAL,CAAsByB,OAAtB,CAA+BC,KAAD,IAAW;QACvC,IAAIC,IAAG,GAAI,EAAX;QACAA,IAAI,CAACC,IAAL,GAAYF,KAAK,CAACE,IAAN,GAAaF,KAAK,CAACE,IAAnB,GAA0B,EAAtC;QACAD,IAAI,CAAClB,KAAL,GAAaiB,KAAK,CAAC5B,IAAnB;QACA6B,IA