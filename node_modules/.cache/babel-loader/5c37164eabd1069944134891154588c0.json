{"ast":null,"code":"import PageNavigationItem from \"./PageNavigationItem.vue\";\nimport AnimationFadeUp from \"./AnimationFadeUp\";\nexport default {\n  name: \"SideBarMenu\",\n\n  data() {\n    return {\n      theme: \"white-theme\",\n      freeLocationMenu: [],\n      menus: [{\n        header: false,\n        title: \"Menu\"\n      }, {\n        href: \"/\",\n        title: \"Dashboard\",\n        icon: \"fa fa-user\"\n      }, {\n        href: \"/button\",\n        title: \"Button\",\n        icon: \"fa fa-chart-area\"\n      }]\n    };\n  },\n\n  components: {\n    PageNavigationItem,\n    AnimationFadeUp\n  },\n  methods: {\n    praiseData() {\n      let menuGroup = [];\n\n      if (!this.freeLocationMenu) {\n        return;\n      }\n\n      this.freeLocationMenu.forEach(group => {\n        let menu = {};\n        menu.icon = group.icon ? group.icon : \"\";\n        menu.title = group.name;\n        menu.url = group.url;\n        menu.items = [];\n        group.menuLocation.forEach(submenu => {\n          let obj = {\n            name: submenu.name,\n            url: submenu.url,\n            isOpenNewTab: submenu.isOpenNewTab,\n            iconName: submenu.iconName,\n            collapse: true\n          };\n\n          if (\"menus\" in submenu) {\n            obj.items = submenu.menus.map(i => {\n              i = { ...i,\n                ...{\n                  collapse: true\n                }\n              };\n              return i;\n            });\n          }\n\n          menu.items.push(obj);\n        });\n        menuGroup.push(menu);\n      });\n      this.menus = menuGroup;\n    },\n\n    async getListSidebar() {\n      // let url = `${window.companyPath}`;\n      let menuSidebar = {\n        meta: {\n          taskCount: 2\n        },\n        data: {\n          title: \"みんなの本棚\",\n          categories: [{\n            name: \"評価が高いランキング\",\n            url: ``\n          }, {\n            name: \"カテゴリー\",\n            menus: [],\n            url: \"\"\n          }, {\n            name: \"新入社員にお勧めの書籍\"\n          }, {\n            name: \"管理職にお勧めの書籍\"\n          }, {\n            name: \"サマリー\",\n            url: `${window.companyPath}bookshelf/main-bookshelf/statistical-page`\n          }]\n        }\n      }; // get sub-category\n      // const response = await api('getListSidebarApi')\n      // this.$store.commit('categories/SET_LIST_CATEGORIES', response.data?.data)\n      // if (response.data.data) {\n      //   menuSidebar.data.categories[1].menus = response.data.data\n      //   menuSidebar.data.categories[1].url = `${window.companyPath}bookshelf/list-book/${response.data.data[0]?.id}`\n      // }\n\n      this.titleSidebar = menuSidebar.data.title;\n      this.freeLocationMenu = menuSidebar.data.categories.map(item => {\n        return {\n          name: \"\",\n          menuLocation: [item]\n        };\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoEA,OAAOA,kBAAP,MAA+B,0BAA/B;AACA,OAAOC,eAAP,MAA4B,mBAA5B;AACA,eAAe;EACbC,IAAI,EAAE,aADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,aADF;MAELC,gBAAgB,EAAE,EAFb;MAGLC,KAAK,EAAE,CACL;QACEC,MAAM,EAAE,KADV;QAEEC,KAAK,EAAE;MAFT,CADK,EAKL;QACEC,IAAI,EAAE,GADR;QAEED,KAAK,EAAE,WAFT;QAGEE,IAAI,EAAE;MAHR,CALK,EAUL;QACED,IAAI,EAAE,SADR;QAEED,KAAK,EAAE,QAFT;QAGEE,IAAI,EAAE;MAHR,CAVK;IAHF,CAAP;EAoBD,CAvBY;;EAwBbC,UAAU,EAAE;IAAEX,kBAAF;IAAsBC;EAAtB,CAxBC;EAyBbW,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,IAAIC,SAAQ,GAAI,EAAhB;;MACA,IAAI,CAAC,KAAKT,gBAAV,EAA4B;QAC1B;MACF;;MACA,KAAKA,gBAAL,CAAsBU,OAAtB,CAA+BC,KAAD,IAAW;QACvC,IAAIC,IAAG,GAAI,EAAX;QACAA,IAAI,CAACP,IAAL,GAAYM,KAAK,CAACN,IAAN,GAAaM,KAAK,CAACN,IAAnB,GAA0B,EAAtC;QACAO,IAAI,CAACT,KAAL,GAAaQ,KAAK,CAACd,IAAnB;QACAe,IAAI,CAACC,GAAL,GAAWF,KAAK,CAACE,GAAjB;QACAD,IAAI,CAACE,KAAL,GAAa,EAAb;QACAH,KAAK,CAACI,YAAN,CAAmBL,OAAnB,CAA4BM,OAAD,IAAa;UACtC,IAAIC,GAAE,GAAI;YACRpB,IAAI,EAAEmB,OAAO,CAACnB,IADN;YAERgB,GAAG,EAAEG,OAAO,CAACH,GAFL;YAGRK,YAAY,EAAEF,OAAO,CAACE,YAHd;YAIRC,QAAQ,EAAEH,OAAO,CAACG,QAJV;YAKRC,QAAQ,EAA