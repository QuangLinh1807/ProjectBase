{"ast":null,"code":"import BaseInput from \"@/components/Input/InputBase.vue\";\nimport BaseButton from \"@/components/Button/BasicButton/BasicButton.vue\";\nexport default {\n  data() {\n    return {\n      submitted: false,\n      fieldEmail: {\n        type: \"email\",\n        placeholder: \"Email\",\n        name: \"email\",\n        color: \"primary\",\n        label: \"Email\",\n        error: false\n      },\n      fieldPassword: {\n        type: \"password\",\n        placeholder: \"Password\",\n        name: \"password\",\n        color: \"primary\",\n        label: \"Password\",\n        error: false\n      },\n      fieldConfirmPassword: {\n        type: \"password\",\n        placeholder: \"ConfirmPassword\",\n        name: \"ConfirmPassword\",\n        color: \"primary\",\n        label: \"Confirm Password\",\n        error: false\n      },\n      registerActive: false,\n      valueEmail: \"\",\n      valuePassWord: \"\",\n      confirmPassWord: \"\"\n    };\n  },\n\n  components: {\n    BaseInput,\n    BaseButton\n  },\n  watch: {\n    valueEmail() {\n      this.checkValidate();\n    },\n\n    valuePassWord() {\n      this.checkValidate();\n    },\n\n    confirmPassWord() {\n      this.checkValidate();\n    }\n\n  },\n  methods: {\n    handleValueEmail(val) {\n      this.valueEmail = val;\n    },\n\n    handleValuePass(val) {\n      this.valuePassWord = val;\n    },\n\n    handleValueConfirmPass(val) {\n      this.confirmPassWord = val;\n    },\n\n    checkValidate() {\n      const regexEmail = /[a-z0-9]+@[a-z]+\\.[a-z]{2,3}/;\n      let isCheckEmail = regexEmail.test(this.valueEmail);\n\n      if (!this.valueEmail || !isCheckEmail) {\n        this.fieldEmail.error = true;\n        this.submitted = true;\n      } else {\n        this.fieldEmail.error = false;\n        this.submitted = false;\n      }\n\n      if (!this.valuePassWord) {\n        this.fieldPassword.error = true;\n        this.submitted = true;\n      } else {\n        this.fieldPassword.error = false;\n        this.submitted = false;\n      }\n\n      if (!this.confirmPassWord || this.confirmPassWord !== this.valuePassWord) {\n        this.fieldConfirmPassword.error = true;\n        this.submitted = true;\n      } else {\n        this.fieldConfirmPassword.error = false;\n        this.submitted = false;\n      }\n    },\n\n    handleActive() {\n      this.registerActive = !this.registerActive;\n    },\n\n    handleSubmit() {\n      let validate = !this.fieldEmail.error && !this.fieldPassword.error;\n\n      if (validate) {\n        console.log(this.valueEmail, this.valuePassWord);\n        this.$router.push({\n          path: `/button`\n        });\n      }\n    },\n\n    handleRegister() {\n      let validate = !this.fieldEmail.error && !this.fieldPassword.error && !this.fieldConfirmPassword.error;\n\n      if (validate) {\n        localStorage.setItem(\"login\", JSON.stringify({\n          email: this.valueEmail,\n          password: this.valuePassWord\n        }));\n        alert(\"đăng kí thành công\");\n        this.registerActive = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8EA,OAAOA,SAAP,MAAsB,kCAAtB;AACA,OAAOC,UAAP,MAAuB,iDAAvB;AACA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,KADN;MAELC,UAAU,EAAE;QACVC,IAAI,EAAE,OADI;QAEVC,WAAW,EAAE,OAFH;QAGVC,IAAI,EAAE,OAHI;QAIVC,KAAK,EAAE,SAJG;QAKVC,KAAK,EAAE,OALG;QAMVC,KAAK,EAAE;MANG,CAFP;MAULC,aAAa,EAAE;QACbN,IAAI,EAAE,UADO;QAEbC,WAAW,EAAE,UAFA;QAGbC,IAAI,EAAE,UAHO;QAIbC,KAAK,EAAE,SAJM;QAKbC,KAAK,EAAE,UALM;QAMbC,KAAK,EAAE;MANM,CAVV;MAkBLE,oBAAoB,EAAE;QACpBP,IAAI,EAAE,UADc;QAEpBC,WAAW,EAAE,iBAFO;QAGpBC,IAAI,EAAE,iBAHc;QAIpBC,KAAK,EAAE,SAJa;QAKpBC,KAAK,EAAE,kBALa;QAMpBC,KAAK,EAAE;MANa,CAlBjB;MA0BLG,cAAc,EAAE,KA1BX;MA2BLC,UAAU,EAAE,EA3BP;MA4BLC,aAAa,EAAE,EA5BV;MA6BLC,eAAe,EAAE;IA7BZ,CAAP;EA+BD,CAjCY;;EAkCbC,UAAU,EAAE;IAAEjB,SAAF;IAAaC;EAAb,CAlCC;EAmCbiB,KAAK,EAAE;IACLJ,UAAU,GAAG;MACX,KAAKK,aAAL;IACD,CAHI;;IAILJ,aAAa,GAAG;MACd,KAAKI,aAAL;IACD,CANI;;IAOLH,eAAe,GAAG;MAChB,KAAKG,aAA